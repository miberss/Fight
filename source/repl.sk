command /repl <code: string>:
    permission: op
    trigger:
        set {_result} to parse effect {_code} from player
        if {_result} = false:
            error("<color:#b02d26>'%{_code}%'</color>")
            stop
        info("<color:#ABCDEF>%player%</color> executed <color:#ABCDEF>'%{_code}%'</color>")